<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iwaki.mysampleapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sample.asynctask.SampleAsyncTaskActivity" />
        <activity android:name=".sample.intentservice.SampleIntentServiceActivity" />

        <service
            android:name=".sample.intentservice.SampleIntentService"
            android:exported="false" />

        <activity android:name=".sample.methodtime.MethodTimeCheckActivity" />
        <activity android:name=".sample.sqlite.SqliteSampleActivity"></activity>
    </application>

</manifest>